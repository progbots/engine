"test.ps" load
"assert.ps" load
"fibonacci.ps" load

"fibonacci" {
  "checks integer" {
    { /a fibonacci } /typecheck assert_throws
  } it

  "0 fibonacci ⇒ 0" {
    0 fibonacci 0 assert_eq
  } it

  "1 fibonacci ⇒ 1" {
    1 fibonacci 1 assert_eq
  } it

  "2 fibonacci ⇒ 1" {
    2 fibonacci 1 assert_eq
  } it

  "3 fibonacci ⇒ 2" {
    3 fibonacci 2 assert_eq
  } it

  "4 fibonacci ⇒ 3" {
    4 fibonacci 3 assert_eq
  } it

  "5 fibonacci ⇒ 5" {
    5 fibonacci 5 assert_eq
  } it
} describe
