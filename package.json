{
  "name": "engine",
  "version": "1.0.0",
  "description": "Main engine",
  "scripts": {
    "pretest": "rimraf .stryker-tmp",
    "test": "jest",
    "lint": "ts-standard --fix",
    "build": "tsc --project tsconfig-build.json && terser dist/engine.js --compress --keep-fnames --mangle -o dist/engine.js",
    "start": "ts-node repl",
    "build:doc": "npm run build && ncp ./dist/engine.js ./docs/repl/engine.js && ts-node utf8toansii.ts docs/repl/engine.js && ncp ./node_modules/systemjs/dist/s.min.js ./docs/repl/systemjs.js && ncp ./node_modules/systemjs/dist/extras/named-register.min.js ./docs/repl/named-register.js",
    "start:doc": "npm run build:doc && reserve",
    "mutate": "stryker run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/progbots/engine.git"
  },
  "author": "Arnaud Buchholz",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/progbots/engine/issues"
  },
  "homepage": "https://github.com/progbots/engine#readme",
  "devDependencies": {
    "@stryker-mutator/core": "^7.0.2",
    "@stryker-mutator/jest-runner": "^7.0.2",
    "@stryker-mutator/typescript-checker": "^7.0.2",
    "@types/jest": "^29.5.2",
    "@typescript-eslint/typescript-estree": "^5.59.11",
    "jest": "^29.5.0",
    "ncp": "^2.0.0",
    "reserve": "^1.15.2",
    "rimraff": "^0.0.1-security",
    "systemjs": "^6.14.1",
    "terser": "^5.18.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "ts-standard": "^12.0.2",
    "typescript": "^5.1.3"
  },
  "ts-standard": {
    "ignore": [
      "dist",
      "docs/repl/**/*.js"
    ]
  }
}
