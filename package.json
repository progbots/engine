{
  "name": "engine",
  "version": "1.0.0",
  "description": "Main engine",
  "scripts": {
    "test": "jest",
    "lint": "ts-standard --fix",
    "build": "tsc --project tsconfig-build.json && uglifyjs dist/engine.js --compress --mangle -o dist/engine.js",
    "start": "ts-node repl",
    "build:doc": "ncp ./dist/engine.js ./docs/repl/engine.js && ncp ./node_modules/systemjs/dist/s.min.js ./docs/repl/systemjs.js && ncp ./node_modules/systemjs/dist/extras/named-register.min.js ./docs/repl/named-register.js",
    "start:doc": "npm run build:doc && reserve"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/progbots/engine.git"
  },
  "author": "Arnaud Buchholz",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/progbots/engine/issues"
  },
  "homepage": "https://github.com/progbots/engine#readme",
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@typescript-eslint/typescript-estree": "^5.58.0",
    "jest": "^29.5.0",
    "ncp": "^2.0.0",
    "reserve": "^1.15.2",
    "systemjs": "^6.14.1",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "ts-standard": "^12.0.2",
    "typescript": "^5.0.4",
    "uglify-js": "^3.17.4"
  },
  "ts-standard": {
    "ignore": [
      "dist",
      "docs/repl/**/*.js"
    ]
  }
}
